{
  "version": 2,
  "name": "mtg-scanner-pro",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app"
}
